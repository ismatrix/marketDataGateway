
{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "globals": {
    "__DEV__": true
  },
  "rules": {
    "no-confusing-arrow": 0,
    "react/jsx-quotes": 0,
    "jsx-quotes": [2, "prefer-double"],
    "no-param-reassign": [2, {"props": false}],
    "no-console": 0,
    "consistent-return": 0,
    "generator-star-spacing": 0,
    "require-yield": 0,
    "no-underscore-dangle": ["error", { "allow": ["_id"] }],
    no-mixed-operators: ["error", {"allowSamePrecedence": true}],
  }
}
